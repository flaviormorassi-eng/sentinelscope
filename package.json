{
  "name": "sentinelscope",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "dotenv -e .env -- tsx server/index.ts",
    "dev:bg": "nohup npm run dev >/tmp/sentinel_dev.log 2>&1 & echo $!",
    "dev:stop": "lsof -tiTCP:3001 -sTCP:LISTEN | xargs -r kill",
    "dev:logs": "tail -f /tmp/sentinel_dev.log",
    "start:bg": "nohup env PORT=3001 NODE_ENV=production node dist/index.js >/tmp/sentinel_prod.log 2>&1 & echo $!",
    "start:logs": "tail -f /tmp/sentinel_prod.log",
    "start:restart": "npm run dev:stop && npm run start:bg",
    "dev:restart": "npm run dev:stop && npm run dev:bg",
    "dev:local": "PORT=3001 JWT_SECRET=devsecret ALLOW_LEGACY_X_USER_ID=true DISABLE_RATE_LIMIT=1 npm run dev",
    "dev:pg": "cp .env.local.postgres .env && docker compose up -d db && npm run db:migrate && npm run dev",
    "dev:auto": "bash ./scripts/dev-auto.sh",
    "pg:reset": "bash ./scripts/pg-reset.sh",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:junit": "vitest run --reporter=default --reporter=junit --outputFile junit-report.xml",
    "coverage:badge": "tsx scripts/generate-coverage-badge.ts",
    "browsing:ingest": "node examples/ingest-browsing.mjs",
    "db:push": "drizzle-kit push",
    "db:manual-init": "dotenv -e .env -- tsx scripts/manual-init-migration.ts",
    "db:migrate": "dotenv -e .env -- tsx scripts/apply-migrations.ts",
    "db:migrate:dry-run": "DRY_RUN=1 dotenv -e .env -- tsx scripts/apply-migrations.ts",
    "db:migrate:list": "dotenv -e .env -- tsx scripts/list-migrations.ts",
    "db:migrate:json": "JSON=1 dotenv -e .env -- tsx scripts/apply-migrations.ts",
    "db:migrate:list:json": "JSON=1 dotenv -e .env -- tsx scripts/list-migrations.ts",
    "db:rollback": "dotenv -e .env -- tsx scripts/rollback-migration.ts",
    "db:rollback:json": "JSON=1 dotenv -e .env -- tsx scripts/rollback-migration.ts",
    "db:migrate:pending:json": "PENDING_ONLY=1 JSON=1 dotenv -e .env -- tsx scripts/list-migrations.ts",
    "db:migrate:mark-changed": "dotenv -e .env -- tsx scripts/migration-mark-changed.ts",
    "db:migrate:new": "dotenv -e .env -- tsx scripts/create-migration.ts",
    "db:migrate-flagged": "node scripts/add-flagged-only-default.mjs",
    "docker:up": "docker compose up -d --build",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f app",
    "quickstart": "bash ./scripts/quickstart.sh",
    "quickstart:win": "powershell -ExecutionPolicy Bypass -File ./scripts/quickstart.ps1",
    "rotation:test": "bash ./scripts/rotation-test.sh",
    "dev:provision": "bash ./scripts/dev-provision.sh",
    "probe:endpoints": "tsx scripts/probe-endpoints.ts"
  },
  "bin": {
    "sentinel-bootstrap": "scripts/bootstrap.ts"
  },
